# rf_power_demand_model.py
from sklearn.ensemble import RandomForestRegressor
import joblib
import numpy as np
import pandas as pd

# Example: Load your training data (replace with actual dataset)
# X_train: features, y_train: target variable
X_train = pd.DataFrame(np.random.rand(100, 10))  # Example feature data
y_train = np.random.rand(100)  # Example target data

# Train a RandomForest model
rf_model = RandomForestRegressor(n_estimators=100)
rf_model.fit(X_train, y_train)

# Save the model as a .pkl file
joblib.dump(rf_model, 'rf_power_demand_model.pkl')
print("Random Forest Model for Power Demand has been saved as 'rf_power_demand_model.pkl'")
