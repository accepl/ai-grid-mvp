# bess_model.py
from sklearn.ensemble import RandomForestRegressor
import joblib
import numpy as np
import pandas as pd

# Example: Load your training data (replace with actual dataset)
# X_train: features, y_train: target variable (e.g., battery charge level)
X_train = pd.DataFrame(np.random.rand(100, 5))  # Example feature data (like energy usage patterns)
y_train = np.random.rand(100)  # Example target data (like battery charge levels)

# Train a RandomForest model for BESS prediction
bess_model = RandomForestRegressor(n_estimators=100)
bess_model.fit(X_train, y_train)

# Save the model as a .pkl file
joblib.dump(bess_model, 'bess_model.pkl')
print("BESS Model has been saved as 'bess_model.pkl'")
